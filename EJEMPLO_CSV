integer f,i;
localparam BITS = 6;
reg [BITS-1:0] x,y;
reg [BITS:0] s;
initial begin
    seed = 1;
    f = $open("output.csv", "w");
    $fwrite(f, "x, y, s \n");
    for (i=0; i<=1000; i++) begin
        x = ($random(seed)%(2**BITS);
        y = ($random(seed)%(2**BITS);
        #1;
        s = x+y;
        $fwrite(f, "%d, %d, %d \n", x, y, s);
end
$fclose(f);
end

